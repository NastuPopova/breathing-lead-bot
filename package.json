{
  "name": "breathing-lead-bot",
  "version": "1.0.0",
  "description": "Лидогенерирующий бот для диагностики дыхательных практик с VERSE-анализом",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest",
    "lint": "eslint .",
    "health-check": "node scripts/health-check.js",
    "debug-stress": "node scripts/debug_stress_level.js",
    "debug-questions": "node scripts/debug_questions.js",
    "debug-full": "npm run debug-stress && npm run health-check",
    "logs": "tail -f logs/bot.log",
    "reset-logs": "rm -f logs/*.log && touch logs/bot.log",
    "deploy": "git push origin main",
    "railway-logs": "railway logs --follow",
    "railway-deploy": "railway up"
  },
  "keywords": [
    "telegram-bot",
    "breathing",
    "wellness",
    "lead-generation",
    "verse-framework",
    "health",
    "stress-management",
    "breathing-practices"
  ],
  "author": {
    "name": "Anastasia Skorodumova",
    "email": "contact@breathingpractices.com"
  },
  "license": "MIT",
  "dependencies": {
    "telegraf": "^4.15.6",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "pdfkit": "^0.15.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "eslint": "^8.55.0",
    "@types/node": "^20.10.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-repo/breathing-lead-bot.git"
  },
  "bugs": {
    "url": "https://github.com/your-repo/breathing-lead-bot/issues"
  },
  "homepage": "https://github.com/your-repo/breathing-lead-bot#readme",
  "directories": {
    "lib": "modules",
    "test": "tests",
    "doc": "docs"
  },
  "files": [
    "index.js",
    "config.js",
    "modules/",
    "scripts/",
    "README.md",
    "LICENSE"
  ]
}
